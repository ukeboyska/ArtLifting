        "text_JBiTgy": {
          "type": "text",
          "settings": {
            "text": "<p>Corporate artwork has become a powerful tool for organizations that want to bring their values to life through human-centered design. ArtLifting connects artists’ stories with the workplace, helping companies foster engagement, belonging, and lasting impact through thoughtfully curated workplace art.</p>",
            "text_size": 4,
            "text_columns": 1
          }
        },
        "spacer_4tDnzQ": {
          "type": "spacer",
          "settings": {
            "spacer_padding": 20,
