<!-- /sections/page.liquid -->
{%- liquid
  assign kicker_line_class = ''
  if section.settings.title == blank
    assign kicker_line_class = 'kicker__line'
  endif

  assign animation_anchor = "[data-section-id='" | append: section.id | append: "']"
-%}


  <section class="page__template section-padding {{ section.settings.text_color }} {{ section.settings.bg }}"
    data-section-id="{{ section.id }}"
    data-section-type="page"
    style="--PT: {{ section.settings.padding_top }}px; --PB: {{ section.settings.padding_bottom }}px;">
    <div class="wrapper section-padding">
      <br><br>
    
    
      <div class="text__standard text-center">
          <h2 class="standard__heading {{ heading_size_class }}"
            data-aos="hero"
            data-aos-anchor="{{ animation_anchor }}"
            data-aos-order="2">
            {{ page.title }}
          </h2>
          <div
            data-aos="hero"
            data-aos-anchor="{{ animation_anchor }}"
            data-aos-order="3">
            <div class="rte {{ font_size_class }}">
              {{ page.content }}
            </div>
          </div>
      </div>
    <div class="desktop" data-organism="locations-picker">
      <br><br>
      <h3>Choose Location</h3>
      <form>
        <select onchange="if (this.value) window.location.href=this.value">
              <option value="-1">select location</option>
           <optgroup label="American Locations">
              {% for link in linklists.locations.links %}
              <option value="{{ link.url | replace: "/pages/", "/collections/" }}">{{ link.title }}</option>
              {% endfor %}
           </optgroup>
           <optgroup label="Canadian Locations">
            {% for link in linklists.canadian-locations.links %}
            <option value="{{ link.url | replace: "/pages/", "/collections/" }}">{{ link.title }}</option>
            {% endfor %}
           </optgroup>
        </select>
        <!--button>View</button-->
      </form>
    </div>
   
