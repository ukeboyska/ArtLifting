        "text_JBiTgy": {
          "type": "text",
          "settings": {
            "text": "<p>Art in the workplace is no longer just about aesthetics. It is a strategic tool for culture, creativity, and connection. When corporate art reflects the people and purpose behind a company, it transforms workplaces into spaces where employees feel inspired to engage.</p>",
            "text_size": 4,
            "text_columns": 1
          }
        },
        "spacer_4tDnzQ": {
          "type": "spacer",
          "settings": {
            "spacer_padding": 20,
